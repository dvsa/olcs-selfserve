{
    "name": "olcs/olcs-selfserve",
    "description": "OLCS Self Service Web Site",
    "repositories": [
        {
            "url": "git@gitlab.clb.npm:olcs/olcs-logging.git",
            "type": "vcs"
        },
        {
            "url": "git@gitlab.clb.npm:olcs/olcs-common.git",
            "type": "vcs"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.clb.npm:sc/jackrabbit.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/MajorCaiger/phpcov.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.clb.npm:olcs/olcs-autoload.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.clb.npm:olcs/olcs-testhelpers.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.clb.npm:cpms/cpms-client.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "2.3.*",
        "doctrine/doctrine-orm-module": "0.*",
        "doctrine/annotations": "1.2.1",
        "olcs/OlcsCommon": "dev-develop",
        "olcs/olcs-logging": "dev-develop",
        "dvsa/jackrabbit": "~1.1.1",
        "symfony/Filesystem": "~2.6.0-dev",
        "olcs/autoload": "dev-develop",
        "cpms/client": "0.1.x-dev"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~1.5.3",
        "phpunit/phpunit": "4.*",
        "sebastian/diff": "dev-master",
        "phpunit/phpcov": "dev-master",
        "mockery/mockery": "0.9.x-dev",
        "mikey179/vfsstream": "dev-master",
        "olcs/olcs-testhelpers": "dev-develop"
    },
    "autoload": {
        "classmap": [
            "module/Olcs/src",
            "vendor/olcs/OlcsCommon/Common/src/Common"
        ],
        "psr-4": {
            "Common\\": "vendor/olcs/OlcsCommon/Common/src/Common"
        }
    },
    "config": {
        "preferred-install": "dist",
        "github-oauth": {
            "github.com": "5cdf40f8d81a7675fdaa5fc92e9381526ef4db7f"
        }
    }
}

{
    "name": "olcs/olcs-selfserve",
    "description": "OLCS Self Service Web Site",
    "repositories": [
        {
            "packagist": false
        },
        {
            "url": "http://devrepo.shd.ci.nonprod.dvsa.aws",
            "type": "composer"
        },
        {
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-logging.git",
            "type": "vcs"
        },
        {
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-common.git",
            "type": "vcs"
        },
        {
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-transfer.git",
            "type": "vcs"
        },
        {
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-utils.git",
            "type": "vcs"
        },
        {
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-auth.git",
            "type": "vcs"
        },
        {
            "type": "vcs",
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-autoload.git"
        },
        {
            "type": "vcs",
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-testhelpers.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "~2.4.0",
        "doctrine/annotations": "^1.2.3",
        "symfony/Filesystem": "~2.6.0",
        "zf-commons/zfc-rbac": "~2.4.0",
        "olcs/autoload": "dev-release/4.23",
        "olcs/OlcsCommon": "dev-release/4.23",
        "olcs/olcs-transfer": "dev-release/4.23",
        "olcs/olcs-utils": "dev-release/4.23",
        "olcs/olcs-auth": "dev-release/4.23",
        "olcs/olcs-logging": "dev-release/4.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0.0",
        "mockery/mockery": "~0.9.3",
        "mikey179/vfsstream": "^1.4.0",
        "olcs/olcs-testhelpers": "dev-develop"
    },
    "autoload": {
        "classmap": [
            "module/Olcs/src",
            "vendor/olcs/OlcsCommon/Common/src/Common"
        ],
        "psr-4": {
            "Common\\": "vendor/olcs/OlcsCommon/Common/src/Common"
        }
    },
    "config": {
        "preferred-install": "dist",
        "secure-http" : false
    },
    "scripts": {
        "ut": "vendor/bin/phpunit -c test/phpunit.xml --stop-on-failure",
        "review": "${dev_workspace}/olcs-devtools/Review.sh",
        "cs": "${dev_workspace}/olcs-devtools/vendor/bin/phpcs --severity=1 --standard=${dev_workspace}/sonar-configuration/Profiles/DVSA/CS/ruleset.xml",
        "cov": "vendor/bin/phpunit -c test/phpunit-coverage.xml",
        "update-syms": "${dev_workspace}/olcs-devtools/composer-update-syms.sh"
    }
}
